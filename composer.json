{
  "name": "vanquybn/yii2-elfinder",
  "description": "Extension to use elFinder 1.x file manager in Yii2 application",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "widget",
    "elFinder",
    "tinymce",
    "file manager"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Quinn Nguyen",
      "email": "quydhnni@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "yiisoft/yii2-jui": "*"
  },
  "suggest": {
    "vanquybn/yii2-tinymce": "Extension with TinyMce WYSIWYG editor, that can be used with elFinder"
  },
  "autoload": {
    "psr-4": {
      "vanquybn\\yii2\\elfinder\\": ""
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "vanquybn/yii2-elfinder",
        "version": "dev-master",
        "source": {
          "url": "git://github.com/vanquybn/yii2-elfinder.git",
          "type": "git",
          "reference": "master"
        },
        "autoload": {
          "psr-0" : {
            "vanquybn\\yii2-elfinder" : ""
          }
        }
      }
    }
  ]
}

